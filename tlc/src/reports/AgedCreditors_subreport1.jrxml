	<queryString>
		<![CDATA[SELECT
    doc.due_date AS documentDate,
    gt.company_unallocated AS companyUnallocated
FROM
    general_transaction AS gt,
    document AS doc
WHERE
    gt.supplier_id = $P{supplierId} AND
    gt.company_unallocated <> 0 AND
    gt.document_id = doc.id]]>
	</queryString>
