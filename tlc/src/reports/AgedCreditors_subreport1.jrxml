	</variable>
	<variable name="totalCurrent" class="java.math.BigDecimal" calculation="Sum">
		<variableExpression><![CDATA[$F{documentDate} > $P{age1Date} ? -$F{companyUnallocated} : 0.0]]></variableExpression>
